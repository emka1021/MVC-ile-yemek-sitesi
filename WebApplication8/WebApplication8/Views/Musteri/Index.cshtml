@using WebApplication8.Models.Entity
@model List<TBLMUSTERI>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
} 

<table class="table table-bordered">
    <tr>
        <th>Müşteri İD</th>
        <th>Müşteri Adı</th>
        <th>Müşteri Soyadı</th>
        <th>SİL</th>
        <th>GÜNCELLE</th>

    </tr>
    <tbody>
        @foreach (var mus in Model)
        {
        <tr>
            <th>@mus.MUSTERIID</th>
            <td>@mus.MUSTERIAD</td>
            <td>@mus.MUSTERISOYAD</td>
            <td><a href="/MUSTERI/SIL/@mus.MUSTERIID" class="btn btn-danger">SİL</a></td>
            <td><a href="/MUSTERI/MusteriGetir/@mus.MUSTERIID" class="btn btn-success">GÜNCELLE</a></td>


        </tr>
        }
    </tbody>
</table>
<a href="/Musteri/YeniMusteri/" class="btn btn-primary">YENİ MUSTERI EKLE</a>

